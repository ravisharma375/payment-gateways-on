<section class="gallery-block cards-gallery">
  <div class="container own-font">
    <div class="heading pt-3">
      <h2 class="own-font ">Our Courses</h2>
    </div>
    <div class="row">
      <% DataCourses.forEach(result=>{ %>
      <div class="col-md-6 col-lg-4">
        <div class="card border-0 transform-on-hover">
          <a class="lightbox" href="<%- result.ImgUrl %>"
            ><img src="<%- result.ImgUrl %>" class="card-img-top " style="height: 230px"
          /></a>
          <div class="card-body">
            <h6><%= result.Name %></h6>
            <p><%- result.Id %></p>
            <p class="text-muted card-text">
              <a href="/productcart?ID=<%- result.Id %>" class="ownpay btn btn-info btn-lg" style="margin-top:5px;"
                >Buy $<%= result.Price %></a
              >
            </p>
          </div>
        </div>
      </div>
      <% }) %>
    </div>
  </div>
</section>


